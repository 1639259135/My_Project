<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <package name="cct" extends="staff">

        <action name="queryAllType" class="courseTypeAction" method="queryAllType">
            <result name="success">/pages/coursetype/listCourse.jsp</result>
        </action>

        <action name="queryByCTID" class="courseTypeAction" method="queryByCTID">
            <result name="success">/pages/coursetype/addOrEditCourse.jsp</result>
        </action>

        <action name="addOrEditCourse" class="courseTypeAction" method="addOrEditCourse">
            <result name="success">/pages/coursetype/listCourse.jsp</result>
        </action>


    </package>

</struts>